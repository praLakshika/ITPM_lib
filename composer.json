{
    "name": "limbcare",
    "description": "Limbcare",
    "type": "project",
    "keywords": ["php", "framework", "laravel", "laravel5", "bootstrap", "boilerplate", "docker", "template", "social-login", "nodejs"],
    "homepage": "https://blasanka@github.com",
    "license": "MIT",
    "authors": [
            {
                "name": "LimbCare",
                "email": "leoshakshared@gmail.com",
                "homepage": "http://www.blasanka.github.com"
            }
        ],
    "require": {
        "php": ">=7.0",
        "albertcht/invisible-recaptcha": "^1.4",
        "arcanedev/log-viewer": "~4.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "barryvdh/laravel-snappy": "^0.4.3",
        "creativeorange/gravatar": "~1.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "fideloper/proxy": "^4.0",
        "fx3costa/laravelchartjs": "^2.5",
        "h4cc/wkhtmltoimage-i386": "0.12.x",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "jimmyjs/laravel-report-generator": "^1.0",
        "jimmyjs/pdf-report-generators": "^1.3",
        "kyslik/column-sortable": "^5.4",
        "labs64/netlicensingclient-php": "^2.3",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "laravelnews/laravel-twbs4": "^1.3",
        "twbs/bootstrap": "^4.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "^2.5",
        "filp/whoops": "~2.0",
        "guzzlehttp/guzzle": "~5.3|~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
